<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	xmlns:waterfall="http://schemas.android.com/apk/res-auto"
	android:orientation="vertical"
	android:background="#ededed">

	<include layout="@layout/core_title_back"/>

	<com.dk.mp.core.widget.ErrorLayout
		android:layout_height="match_parent"
		android:layout_width="match_parent"
		android:id="@+id/error_layout"
		android:gravity="center_horizontal"/>

	<com.dk.mp.xyfg.view.org.WaterfallSmartView
		android:id="@+id/waterfall"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="10dp"
		waterfall:column="2"
		waterfall:itemMargin="10dp" >

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal" >
		</LinearLayout>

	</com.dk.mp.xyfg.view.org.WaterfallSmartView>
</LinearLayout>